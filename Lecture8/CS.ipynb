   "source": [
    "# dimension\n",
    "n = 64\n",
    "# plaintext modulus\n",
    "p = 257\n",
    "# ciphertext modulus\n",
    "q = 1048583\n",
    "\n",
    "V = VectorSpace(GF(q), n)\n",
    "S = V.random_element()\n",
    "\n",
    "\n",
    "def encrypt(m):\n",
    "    A = V.random_element()\n",
    "    e = randint(-1, 1)\n",
    "    b = A * S + m + p * e\n",
    "    return A, b"
   ]
